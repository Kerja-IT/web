generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Job {
    id          String @id @default(cuid())
    url         String
    title       String
    companyName String
    location    String

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
